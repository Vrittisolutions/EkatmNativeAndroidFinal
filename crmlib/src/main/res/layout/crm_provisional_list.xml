    android:layout_height="wrap_content"
    >
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/header"
    android:padding="5dp"
    >
    <TextView
        android:id="@+id/txt_provisional_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
