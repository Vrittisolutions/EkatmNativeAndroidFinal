package com.vritti.vwblib.Beans;

/**
 * Created by sharvari on 5/10/2016.
 */
public class NotificationBean {


    String notification, date, notificationid, vendorid, type;
    String PKNotifDtlsId,
            NotifText, NotifTitle,
            NotificationTypeId, TypeName, UserName, AddedDt;


    public NotificationBean(String notification, String type) {
        this.notification = notification;
        this.type = type;
    }

    public NotificationBean() {
    }

    public String getPKNotifDtlsId() {
        return PKNotifDtlsId;
    }

    public void setPKNotifDtlsId(String PKNotifDtlsId) {
        this.PKNotifDtlsId = PKNotifDtlsId;
    }

    public String getNotifText() {
        return NotifText;
    }

    public void setNotifText(String notifText) {
        NotifText = notifText;
    }

    public String getNotifTitle() {
        return NotifTitle;
    }

    public void setNotifTitle(String notifTitle) {
        NotifTitle = notifTitle;
    }

    public String getNotificationTypeId() {
        return NotificationTypeId;
    }

    public void setNotificationTypeId(String notificationTypeId) {
        NotificationTypeId = notificationTypeId;
    }

    public String getTypeName() {
        return TypeName;
    }

    public void setTypeName(String typeName) {
        TypeName = typeName;
    }

    public String getUserName() {
        return UserName;
    }

    public void setUserName(String userName) {
        UserName = userName;
    }

    public String getAddedDt() {
        return AddedDt;
    }

    public void setAddedDt(String addedDt) {
        AddedDt = addedDt;
    }
    public String getType() {
        return type;
    }

    public void setType(String type) {
        this.type = type;
    }

    public String getNotification() {
        return notification;
    }

    public void setNotification(String notification) {
        this.notification = notification;
    }

    public String getDate() {
        return date;
    }

    public void setDate(String date) {
        this.date = date;
    }

    public String getNotificationid() {
        return notificationid;
    }

    public void setNotificationid(String notificationid) {
        this.notificationid = notificationid;
    }

    public String getVendorid() {
        return vendorid;
    }

    public void setVendorid(String vendorid) {
        this.vendorid = vendorid;
    }
}
