<android.support.design.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:gpb="http://schemas.android.com/apk/res-auto"
    android:fitsSystemWindows="true"

    tools:context="vcrm7.vritti.com.vcrm7.ProspectEnterpriseActivity">

    <android.support.design.widget.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/AppTheme.NoActionBar.AppBarOverlay">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <android.support.v7.widget.Toolbar
                android:id="@+id/toolbar"
                android:layout_width="match_parent"
                android:layout_height="?attr/actionBarSize"
                android:background="?attr/colorPrimary"
                app:theme="@style/ToolbarStyle" />
         </RelativeLayout>

    </android.support.design.widget.AppBarLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/lay1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="?attr/actionBarSize"
            android:background="#6E6F72"
            android:orientation="horizontal"
            android:padding="5dp">


            <TextView

                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:text=" Add Prospect Information"
                android:textColor="#ffffff"
                android:textStyle="bold"
                android:padding="5dp"
                android:textSize="18dp" />
        </LinearLayout>


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#ffffff"
            android:orientation="vertical">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"

                android:layout_marginBottom="60sp"
                android:layout_marginLeft="10sp"
                android:layout_marginRight="10sp"
                android:layout_marginTop="5sp"
                android:orientation="vertical">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    android:background="#0b6bb7">

                    <TextView
                        android:id="@+id/headName"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:layout_centerVertical="true"
                        android:gravity="center"
                        android:paddingLeft="5sp"
                        android:text="Name and Address"
                        android:textColor="#ffffff"
                        android:textSize="16sp" />

                </RelativeLayout>

                <LinearLayout
                    android:id="@+id/llName"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/border"
                    android:orientation="vertical"
                    android:paddingLeft="3sp"
                    android:paddingRight="3sp"
                 >

                     <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                         >

                                <AutoCompleteTextView
                                    android:id="@+id/efName1"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:completionThreshold="1"
                                    android:textColor="#000"
                                    android:hint="Firm name"
                                    android:paddingLeft="10dp"
                                    android:textSize="13sp">

                                    <requestFocus />
                                </AutoCompleteTextView>


                                <ProgressBar
                                    style="?android:attr/progressBarStyleSmall"
                                    android:layout_width="30dp"
                                    android:layout_height="30dp"
                                    android:layout_marginLeft="5dp"
                                    android:layout_weight="0.1"
                                    android:visibility="gone" />
                            </LinearLayout>

                    <EditText
                        android:id="@+id/efAlias"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:textColor="#000000"
                        android:textSize="13sp"
                        android:paddingLeft="10dp"
                        android:hint="Firm alias"/>

                    <EditText
                                android:id="@+id/eAddress"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:hint="Address"
                                android:textColor="#000000"
                                 android:paddingLeft="10dp"
                                android:textSize="13sp" />

                            <AutoCompleteTextView
                                android:id="@+id/eAutoCity"
                                android:layout_width="match_parent"
                                android:layout_height="35dp"
                                android:hint="City"
                                android:textColor="#000000"
                                android:textSize="13sp"
                                android:paddingLeft="10dp"
                                />

                            <AutoCompleteTextView
                                android:id="@+id/eAutoTerritory"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:hint="Territory"
                                android:textColor="#000000"
                                android:textSize="13sp"
                                android:paddingLeft="10dp"
                                />

                            <EditText
                                android:id="@+id/eEmail"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:hint="Email"
                                android:inputType="textEmailAddress"
                                android:textColor="#000000"
                                android:textSize="13sp"
                                android:paddingLeft="10dp"
                                />

                            <EditText
                                android:id="@+id/eUrl"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:hint="Company URL"
                                android:textColor="#000000"
                                android:paddingLeft="10dp"
                                android:textSize="13sp" />

                            <EditText
                                android:id="@+id/eMobile"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:hint="Mobile"
                                android:inputType="number"
                                android:textColor="#000000"
                                android:textSize="13sp"
                                android:paddingLeft="10dp"
                                />

                            <EditText
                                android:id="@+id/eTelephone"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:hint="Telephone"
                                android:inputType="number"
                                android:textColor="#000000"
                                android:textSize="13sp"
                                android:paddingLeft="10dp"
                                />

                            <EditText
                                android:id="@+id/efax"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:hint="Fax"
                                android:textColor="#000000"
                                android:textSize="13sp"
                                android:paddingLeft="10dp"
                                />

                        </LinearLayout>


                <ScrollView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="vertical"
                        android:weightSum="1">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:padding="2dp"
                            android:background="#0b6bb7">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentLeft="true"
                                android:layout_alignParentStart="true"
                                android:layout_centerVertical="true"
                                android:gravity="center"
                                android:paddingLeft="5sp"
                                android:text="Attach Entity"
                                android:textColor="#ffffff"
                                android:textSize="16sp" />

                            <ImageView
                                android:id="@+id/ExpandEntity"
                                android:layout_width="25dp"
                                android:layout_height="25dp"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="3sp"
                                android:background="@drawable/ic_add"
                                android:onClick="EntityClick" />
                        </RelativeLayout>

                        <LinearLayout
                            android:id="@+id/llEntity"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:background="@drawable/border"
                            android:orientation="vertical"
                            android:paddingLeft="3sp"
                            android:paddingRight="3sp"
                            android:visibility="gone">


                            <Spinner
                                android:id="@+id/sEntity"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"

                                android:textColor="#000000"
                                android:textSize="13sp" />

                            <Spinner
                                android:id="@+id/sConsignee"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp"
                                android:textColor="#000000"
                                android:textSize="13sp" />
                        </LinearLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="10dp"
                            android:padding="2dp"
                            android:background="#0b6bb7">

                            <TextView
                                android:id="@+id/headProductServices"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentLeft="true"
                                android:layout_alignParentStart="true"
                                android:layout_centerVertical="true"
                                android:gravity="center"
                                android:paddingLeft="5sp"
                                android:text="Prospective Product Services"
                                android:textColor="#ffffff"
                                android:textSize="16sp" />

                            <ImageView
                                android:id="@+id/ExpandProduct"
                                android:layout_width="25dp"
                                android:layout_height="25dp"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="3sp"
                                android:background="@drawable/ic_add"
                                android:onClick="ProductClick" />
                        </RelativeLayout>

                        <LinearLayout
                            android:id="@+id/llProduct"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/border"
                            android:orientation="vertical"
                            android:paddingLeft="3sp"
                            android:paddingRight="3sp"
                            android:visibility="gone">

                            <EditText
                                android:id="@+id/Products"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:clickable="true"
                                android:hint="--Select Product--"
                                android:textSize="13sp" />

                            <LinearLayout
                                android:id="@+id/llProductList"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginBottom="8sp"
                                android:layout_marginLeft="5sp"
                                android:layout_marginRight="5sp"
                                android:layout_marginTop="10dp"
                                android:orientation="vertical"></LinearLayout>
                        </LinearLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center"
                            android:layout_marginTop="10dp"
                            android:padding="2dp"
                            android:background="#0b6bb7"
                            android:gravity="center">

                            <ImageView
                                android:id="@+id/ExpandBusinessDetails"
                                android:layout_width="25dp"
                                android:layout_height="25dp"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="3sp"
                                android:background="@drawable/ic_add"
                                android:onClick="ExpandBusinessDetailsClick" />

                            <TextView
                                android:id="@+id/headBusiness"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentLeft="true"

                                android:layout_alignParentStart="true"
                                android:layout_centerVertical="true"
                                android:gravity="center"
                                android:paddingLeft="5sp"
                                android:text="Business Details"
                                android:textColor="#ffffff"
                                android:textSize="16sp" />
                        </RelativeLayout>

                        <LinearLayout
                            android:id="@+id/llBusinessDetails"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/border"
                            android:orientation="vertical"
                            android:paddingLeft="3sp"
                            android:paddingRight="3sp"
                            android:visibility="gone">

                            <AutoCompleteTextView
                                android:id="@+id/autotxtBusinessSegment"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp"
                                android:textColor="#000000"
                                android:textSize="13sp" />

                            <EditText
                                android:id="@+id/eEmployeeStrength"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp"
                                android:hint="No. of Employee"
                                android:inputType="number"
                                android:textColor="#000000"
                                android:textSize="13sp" />

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp"
                                android:orientation="horizontal"
                                android:weightSum="1">

                                <TextView

                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="10dp"
                                    android:layout_weight="0.3"
                                    android:drawablePadding="70sp"
                                    android:hint="Currency"
                                    android:textColor="#000000"
                                    android:textSize="13sp" />

                                <Spinner
                                    android:id="@+id/sCurrency"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:layout_weight="0.4"
                                    android:gravity="center"
                                    android:textColor="#000000"
                                    android:textSize="13sp" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp"
                                android:orientation="horizontal"
                                android:weightSum="1">

                                <EditText
                                    android:id="@+id/eTurnover"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="0.6"
                                    android:drawablePadding="70sp"
                                    android:hint="Turnover"
                                    android:inputType="numberDecimal|numberSigned"
                                    android:textColor="#000000"
                                    android:textSize="13sp" />

                                <Spinner
                                    android:id="@+id/sTurnover"
                                    android:layout_width="0dp"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="center"
                                    android:layout_weight="0.4"
                                    android:gravity="center"

                                    android:textColor="#000000"
                                    android:textSize="13sp" />
                            </LinearLayout>

                            <EditText
                                android:id="@+id/eOfficeCnt"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp"
                                android:drawablePadding="85sp"
                                android:hint="No. of offices"
                                android:inputType="number"
                                android:textColor="#000000"
                                android:textSize="13sp" />

                            <EditText
                                android:id="@+id/eRemark"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp"
                                android:hint="More Information"
                                android:inputType="textMultiLine"
                                android:textColor="#000000"
                                android:textSize="13sp" />
                        </LinearLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:padding="2dp"
                            android:background="#0b6bb7">

                            <TextView
                                android:id="@+id/headProspect"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentLeft="true"
                                android:layout_alignParentStart="true"
                                android:layout_centerVertical="true"
                                android:gravity="center"
                                android:paddingLeft="5sp"
                                android:text="Source of Prospect"
                                android:textColor="#ffffff"
                                android:textSize="16sp" />

                            <ImageView
                                android:id="@+id/ExpandProspect"
                                android:layout_width="25dp"
                                android:layout_height="25dp"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="3sp"
                                android:background="@drawable/ic_add"
                                android:onClick="ProspectClick" />
                        </RelativeLayout>

                        <LinearLayout
                            android:id="@+id/llProspect"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:background="@drawable/border"
                            android:orientation="vertical"
                            android:paddingLeft="3sp"
                            android:paddingRight="3sp"
                            android:visibility="gone">

                            <AutoCompleteTextView
                                android:id="@+id/autotxtProspect"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:hint="Source of Prospect"
                                android:textColor="#000000"
                                android:textSize="13sp" />

                            <Spinner
                                android:id="@+id/sReferenceType"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp"
                                android:textColor="#000000"
                                android:textSize="13sp" />

                            <Spinner
                                android:id="@+id/sReference"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp"
                                android:textColor="#000000"
                                android:textSize="13sp" />
                        </LinearLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:padding="2dp"
                            android:background="#0b6bb7">

                            <TextView
                                android:id="@+id/headContact"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentLeft="true"
                                android:layout_alignParentStart="true"
                                android:layout_centerVertical="true"
                                android:gravity="center"
                                android:paddingLeft="5sp"
                                android:text="Contacts"
                                android:textColor="#ffffff"
                                android:textSize="16sp" />

                            <ImageView
                                android:id="@+id/ExpandContact"
                                android:layout_width="25dp"
                                android:layout_height="25dp"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="3sp"
                                android:background="@drawable/ic_add"
                                android:onClick="ContactClick" />
                        </RelativeLayout>

                        <LinearLayout
                            android:id="@+id/llContact"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="@drawable/border"
                            android:orientation="vertical"
                            android:paddingLeft="3sp"
                            android:paddingRight="3sp"
                            android:visibility="gone">

                            <LinearLayout
                                android:id="@+id/llAddContact"

                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <EditText
                                    android:id="@+id/eContactName"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="10dp"
                                    android:hint="Contact Name"
                                    android:textColor="#000000"
                                    android:textSize="13sp" />

                                <EditText
                                    android:id="@+id/eContactDesignation"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="10dp"
                                    android:hint="Designation"
                                    android:textColor="#000000"
                                    android:textSize="13sp" />

                                <Spinner
                                    android:id="@+id/sDepartment"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="10dp"
                                    android:textColor="#000000"
                                    android:textSize="13sp" />

                                <EditText
                                    android:id="@+id/eBirthdate"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="10dp"
                                    android:clickable="true"
                                    android:hint="Birthdate"
                                    android:textColor="#000000"
                                    android:textSize="13sp" />

                                <EditText
                                    android:id="@+id/econtactEmailid"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="10dp"
                                    android:hint="Email"
                                    android:textColor="#000000"
                                    android:textSize="13sp" />

                                <EditText
                                    android:id="@+id/econtactMobile"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="10dp"
                                    android:hint="Mobile Number"
                                    android:inputType="number"
                                    android:maxLength="10"
                                    android:textColor="#000000"
                                    android:textSize="13sp" />

                                <EditText
                                    android:id="@+id/eContactTele"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="10dp"
                                    android:hint="Telephone"
                                    android:inputType="number"
                                    android:textColor="#000000"
                                    android:textSize="13sp" />

                                <EditText
                                    android:id="@+id/eContactFax"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="10dp"
                                    android:hint="Fax"
                                    android:textColor="#000000"
                                    android:textSize="13sp" />

                                <TextView
                                    android:id="@+id/btnSaveContact"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="right"
                                    android:layout_marginTop="10sp"
                                    android:background="@color/btnsave"
                                    android:clickable="true"
                                    android:gravity="right"
                                    android:padding="10dp"
                                    android:text="Save Contact"
                                    android:layout_marginBottom="5dp"
                                    android:layout_marginRight="5dp"
                                    android:textColor="#fff"
                                    android:textSize="14sp" />
                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/llAddContact1"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp"

                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/btnSaveContact1"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_gravity="right"
                                    android:background="@color/btnNext"
                                    android:clickable="true"
                                    android:gravity="right"
                                    android:padding="10dp"
                                    android:text="Add Contact"
                                    android:textColor="#fff"
                                    android:textSize="15sp" />

                                <ListView
                                    android:id="@+id/listContacts"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="10dp"
                                    android:divider="@color/barDarkColor"
                                    android:dividerHeight="1sp"
                                    android:isScrollContainer="false"></ListView>
                            </LinearLayout>
                        </LinearLayout>

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:padding="2dp"
                            android:background="#0b6bb7">

                            <TextView
                                android:id="@+id/headNotes"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentLeft="true"
                                android:layout_alignParentStart="true"
                                android:layout_centerVertical="true"
                                android:gravity="center"
                                android:paddingLeft="5sp"
                                android:text="Notes"
                                android:textColor="#ffffff"
                                android:textSize="16sp" />

                            <ImageView
                                android:id="@+id/ExpandNotes"
                                android:layout_width="25dp"
                                android:layout_height="25dp"
                                android:layout_alignParentRight="true"
                                android:layout_marginRight="3sp"
                                android:background="@drawable/ic_add"
                                android:onClick="NotesClick" />
                        </RelativeLayout>

                        <LinearLayout
                            android:id="@+id/llNotes"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:background="@drawable/border"
                            android:orientation="vertical"
                            android:paddingLeft="3sp"
                            android:paddingRight="3sp"
                            android:visibility="gone">

                            <EditText
                                android:id="@+id/eNotes"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:hint="Remark"
                                android:inputType="textMultiLine"
                                android:textColor="#000000"
                                android:textSize="13sp" />
                        </LinearLayout>
                    </LinearLayout>
                </ScrollView>
            </LinearLayout>
            <com.jpardogo.android.googleprogressbar.library.GoogleProgressBar
                android:id="@+id/progressbar"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_gravity="center"
                android:layout_centerVertical="true"
                android:layout_centerHorizontal="true"
                gpb:colors="@array/google_colors"
                gpb:type="chrome_floating_circles"
                android:visibility="gone"
                />

            <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:layout_alignParentBottom="true"


                    >

                    <Button
                        android:id="@+id/buttonSave_prospect"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginRight="5dp"
                        android:background="@color/btnsave"
                        android:text="Save"
                        android:layout_weight="1"
                        android:layout_marginLeft="5dp"
                        android:textAllCaps="false"
                        android:textColor="@color/white" />

                    <Button
                        android:id="@+id/buttonSaveandstartcall_prospect"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/btnsave"
                        android:text="@string/savecall"
                        android:textAllCaps="false"
                        android:textColor="@color/white"
                        android:layout_weight="1"
                        />

                    <Button
                        android:id="@+id/buttonClose_prospect"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="5dp"
                        android:background="@color/white"
                        android:textAllCaps="false"
                        android:layout_marginRight="5dp"
                        android:text="Cancel"
                        android:layout_weight="1"
                        />
                </LinearLayout>
