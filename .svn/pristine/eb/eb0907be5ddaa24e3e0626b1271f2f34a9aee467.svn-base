                android:layout_height="wrap_content"
                android:layout_weight="0.6"
                android:text="Hot"
                android:textColor="#fff" />

            <TextView
                android:id="@+id/tv_hot_call"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center|end"
                android:layout_weight="0.2"
                android:text="0"
                android:textColor="#fff" />

            <TextView
                android:id="@+id/tv_hot_call_value"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="right|end"
                android:layout_weight="0.2"
                android:text="0"
                android:textColor="#fff" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/lay_warm_call"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="?android:attr/selectableItemBackground"
            android:orientation="horizontal"
            android:paddingBottom="2dp"
            android:paddingLeft="2dp"
            android:paddingTop="2dp">

            <TextView
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.6"
                android:text="Warm"
                android:textColor="#fff" />

            <TextView
                android:id="@+id/tv_warm_call"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center|end"
                android:layout_weight="0.2"
                android:text="0"
                android:textColor="#fff" />

            <TextView
                android:id="@+id/tv_warm_call_value"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_gravity="right|end"
                android:layout_weight="0.2"
                android:text="0"
                android:textColor="#fff" />
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="#292E3E"></View>

        <LinearLayout
            android:id="@+id/ly_team"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center|left"
            android:gravity="center|left"
            android:orientation="horizontal"
            android:visibility="gone"
            android:weightSum="1">

            <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="5dp"
                android:src="@drawable/ic_team" />

            <TextView
                android:id="@+id/txtteam"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.97"
                android:padding="5dp"
                android:text="  Team"
                android:textColor="@color/team"
                android:textSize="14sp"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center|left"
                android:layout_marginRight="5dp"
                android:gravity="center|left"

                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tv_team_mem_cnt"
                    android:layout_width="40dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/team_border_text"
                    android:gravity="center"
                    android:text=""
                    android:textColor="#fff" />

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/icon_plus" />

                <TextView
                    android:id="@+id/tv_team_mem_cnt1"
                    android:layout_width="40dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/team_border_text"
                    android:gravity="center"
                    android:text=""
                    android:textColor="#fff" />
            </LinearLayout>
        </LinearLayout>


        <ListView
            android:id="@+id/ls_Team"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        </ListView>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="#292E3E"></View>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:visibility="gone">

            <TextView
                android:id="@+id/txt_target_achivment"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="5dp"
                android:text="Target Achievement"
                android:textColor="@color/white"
                android:textSize="14sp"
                android:textStyle="normal" />

        </LinearLayout>


    </LinearLayout>
    </ScrollView>
</RelativeLayout>