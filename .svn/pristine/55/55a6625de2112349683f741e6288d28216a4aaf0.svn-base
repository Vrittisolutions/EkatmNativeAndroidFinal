<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:gpb="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context="com.vritti.crm.vcrm7.ProspectEnterpriseActivity">

    <RelativeLayout
        android:id="@+id/rel_pros"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar"
            style="@style/MyToolbar"
            android:layout_width="match_parent"
            android:layout_height="?android:attr/actionBarSize"
            android:background="@color/colorPrimary"
            app:popupTheme="@style/AppTheme.NoActionBar.PopupOverlay"
            app:subtitleTextAppearance="@style/ToolbarTextAppearance.Subtitle"
            app:titleTextAppearance="@style/ToolbarTextAppearance.Title"
            app:contentInsetLeft="0dp"
            app:contentInsetStart="0dp"
            app:contentInsetStartWithNavigation="0dp">

            <ProgressBar
                android:id="@+id/progressbar"
                style="?android:attr/progressBarStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_gravity="right"
                android:indeterminateDrawable="@drawable/spinner"
                android:visibility="gone" />
        </android.support.v7.widget.Toolbar>
    </RelativeLayout>

    <LinearLayout
        android:id="@+id/lay1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#6E6F72"
        android:layout_below="@+id/rel_pros"
        android:orientation="horizontal"
        android:padding="5dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical|left"
            android:padding="5dp"
            android:layout_weight="0.8"
            android:text="Create  Prospect Enterprise"
            android:textColor="#ffffff"
            android:textSize="16dp"
            android:textStyle="bold" />

        <ImageView
            android:id="@+id/img_DatasheetForms"
            android:layout_width="35dp"
            android:layout_height="35dp"
            android:layout_gravity="center_vertical|right"
            android:layout_weight="0.2"
            android:layout_marginRight="5dp"
            android:src="@drawable/document" />
    </LinearLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/lay1"
        android:layout_above="@+id/lay2"
        android:scrollbars="none">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusableInTouchMode="true"
            android:orientation="vertical"
            android:layout_margin="3dp">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#ffffff"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:focusableInTouchMode="true"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="#6fa2ca"
                        android:orientation="horizontal"
                        android:padding="5dp">

                        <TextView
                            android:id="@+id/headName"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:text="Name and Address"
                            android:textSize="15sp"
                            android:textColor="@color/white"
                            android:textStyle="bold"/>

                        <View
                            android:layout_width="match_parent"
                            android:layout_height="1dp"
                            android:background="#6fa2ca"
                            android:visibility="gone"/>

                    </LinearLayout>


                    <!--lay name one-->
                    <LinearLayout
                        android:id="@+id/llName"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:focusableInTouchMode="true"
                        android:orientation="vertical"
                        android:visibility="visible"
                        android:layout_marginLeft="5dp"
                        android:layout_marginRight="5dp">
                        <!--firmname-->
                        <LinearLayout
                            android:id="@+id/ln_firmname"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:focusableInTouchMode="true"
                            android:orientation="horizontal"
                            android:visibility="visible">

                            <android.support.design.widget.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:hint="Firm Name">

                                <AutoCompleteTextView
                                    android:id="@+id/atxt_firmname"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:imeOptions="actionNext"
                                    android:singleLine="true"
                                    android:textColorHint="@color/hint"
                                    android:textColor="#111"
                                    android:textSize="12sp">

                                    <requestFocus />
                                </AutoCompleteTextView>

                            </android.support.design.widget.TextInputLayout>

                            <ProgressBar
                                style="?android:attr/progressBarStyleSmall"
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                android:layout_marginLeft="5dp"
                                android:layout_weight="0.1"
                                android:visibility="gone" />
                        </LinearLayout>

                        <!--firm alias-->

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <android.support.design.widget.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:hint="Firm Alias">

                                <EditText
                                    android:id="@+id/edt_firmalise"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:imeOptions="actionNext"
                                    android:singleLine="true"
                                    android:textColor="#111"
                                    android:textColorHint="@color/hint"
                                    android:textSize="12sp"
                                    android:visibility="visible" />

                            </android.support.design.widget.TextInputLayout>

                        </LinearLayout>

                        <!--attach entity-->
                        <RelativeLayout
                            android:id="@+id/rl_entity"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <android.support.v7.widget.AppCompatCheckBox
                                android:id="@+id/checkbox_entity"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_alignParentLeft="true"
                                android:layout_alignParentStart="true"
                                android:layout_centerVertical="true"
                                android:buttonTint="@color/colorPrimary"
                                android:gravity="center|left"
                                android:checked="false"
                                android:text="Attach Entity"
                                android:textColor="@color/black"
                                android:textSize="12sp"
                                android:textStyle="normal" />
                        </RelativeLayout>


                        <LinearLayout
                            android:id="@+id/ln_Entity"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:focusableInTouchMode="true"
                            android:orientation="vertical"
                            android:visibility="gone">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:focusableInTouchMode="true"
                                android:orientation="horizontal">

                                <android.support.design.widget.TextInputLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">

                                    <AutoCompleteTextView
                                        android:id="@+id/sEntity"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:textColor="#111"
                                        android:textSize="12sp" />

                                </android.support.design.widget.TextInputLayout>


                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:focusableInTouchMode="true"
                                android:orientation="horizontal"
                                android:visibility="gone">

                                <android.support.design.widget.TextInputLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">

                                    <AutoCompleteTextView
                                        android:id="@+id/sConsignee"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:textColor="#111"
                                        android:textSize="12sp" />
                                </android.support.design.widget.TextInputLayout>


                            </LinearLayout>

                        </LinearLayout>

                        <android.support.design.widget.TextInputLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <EditText
                                android:id="@+id/edt_Address"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:hint="Full Address "
                                android:inputType="textMultiLine"
                                android:singleLine="true"
                                android:text=""
                                android:textColor="#111"
                                android:textSize="12sp"
                                android:visibility="visible" />

                        </android.support.design.widget.TextInputLayout>

                        <!--country state-->
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <!--country-->
                            <LinearLayout
                                android:id="@+id/ln_country"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="horizontal"
                                android:visibility="visible">

                                <android.support.design.widget.TextInputLayout
                                    android:id="@+id/tv_country"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">

                                    <AutoCompleteTextView
                                        android:id="@+id/spinner_country"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:hint="Country"
                                        android:imeOptions="actionNext"
                                        android:singleLine="false"
                                        android:textColor="#111"
                                        android:textColorHint="@color/hint"
                                        android:textSize="12dp"
                                        android:clickable="true"
                                        android:editable="false"
                                        android:focusable="false"/>

                                </android.support.design.widget.TextInputLayout>

                            </LinearLayout>

                            <!--state-->
                            <LinearLayout
                                android:id="@+id/ln_state"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center|left"
                                android:layout_weight="1"
                                android:gravity="center|left"
                                android:orientation="horizontal"
                                android:visibility="visible">

                                <android.support.design.widget.TextInputLayout
                                    android:id="@+id/tv_state"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">

                                    <AutoCompleteTextView
                                        android:id="@+id/spinner_state"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:spinnerMode="dialog"
                                        android:hint="State"
                                        android:textColor="#111"
                                        android:singleLine="false"
                                        android:textSize="12sp"
                                        android:clickable="true"
                                        android:editable="false"
                                        android:focusable="false"/>

                                </android.support.design.widget.TextInputLayout>

                            </LinearLayout>

                        </LinearLayout>

                        <!--city terriotory-->
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <!--city-->
                            <LinearLayout
                                android:id="@+id/ln_city"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="horizontal"
                                android:visibility="visible">

                                <android.support.design.widget.TextInputLayout
                                    android:id="@+id/tv_city"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">

                                    <AutoCompleteTextView
                                        android:id="@+id/spinner_City"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:imeOptions="actionNext"
                                        android:singleLine="false"
                                        android:textColor="@color/black"
                                        android:textColorHint="@color/hint"
                                        android:textSize="12dp"
                                        android:hint="City"
                                        android:clickable="true"
                                        android:editable="false"
                                        android:focusable="false"/>

                                </android.support.design.widget.TextInputLayout>
                            </LinearLayout>

                            <!--territory-->
                            <LinearLayout
                                android:id="@+id/ln_territory"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="horizontal"
                                android:visibility="visible">

                                <android.support.design.widget.TextInputLayout
                                    android:id="@+id/tv_territory"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">

                                    <AutoCompleteTextView
                                        android:id="@+id/spinner_territory"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:hint="Territory"
                                        android:imeOptions="actionNext"
                                        android:layout_gravity="center_vertical"
                                        android:singleLine="true"
                                        android:textColor="@color/black"
                                        android:textColorHint="@color/hint"
                                        android:textSize="12dp"
                                        android:clickable="true"
                                        android:editable="false"
                                        android:focusable="false"/>

                                </android.support.design.widget.TextInputLayout>

                            </LinearLayout>

                        </LinearLayout>

                        <!--GSTN TAN NO-->
                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal"
                                android:visibility="visible">

                                <!--GSTN-->
                                <android.support.design.widget.TextInputLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">

                                    <EditText
                                        android:id="@+id/edt_GSTN"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:digits="ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz1234567890"
                                        android:hint="GSTN"
                                        android:imeOptions="actionNext"
                                        android:inputType="text"
                                        android:maxLength="15"
                                        android:singleLine="true"
                                        android:text=""
                                        android:textColor="#111"
                                        android:textColorHint="@color/hint"
                                        android:textSize="12sp"
                                        android:visibility="visible" />

                                </android.support.design.widget.TextInputLayout>
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="horizontal"
                                android:visibility="visible">

                                <!--TANNO-->
                                <android.support.design.widget.TextInputLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">

                                    <EditText
                                        android:id="@+id/edt_TANNO"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:hint="TAN No."
                                        android:digits="ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz1234567890"
                                        android:imeOptions="actionNext"
                                        android:singleLine="true"
                                        android:text=""
                                        android:textColor="#111"
                                        android:textColorHint="@color/hint"
                                        android:textSize="12sp"
                                        android:visibility="visible" />

                                </android.support.design.widget.TextInputLayout>
                            </LinearLayout>

                        </LinearLayout>

                        <!--tan no name-->
                        <android.support.design.widget.TextInputLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <EditText
                                android:id="@+id/edt_TANNONAME"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:hint="TAN No. Name"
                                android:imeOptions="actionNext"
                                android:singleLine="true"
                                android:text=""
                                android:textColor="#111"
                                android:textColorHint="@color/hint"
                                android:textSize="12sp"
                                android:visibility="visible" />

                        </android.support.design.widget.TextInputLayout>

                        <!--business segment-->
                        <LinearLayout
                            android:id="@+id/ln_businessSegment"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center|left"
                            android:layout_weight="1"
                            android:gravity="center|left"
                            android:orientation="horizontal"
                            android:visibility="visible">

                            <android.support.design.widget.TextInputLayout
                                android:id="@+id/tv_businessSegment"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                                <AutoCompleteTextView
                                    android:id="@+id/spinner_BusinessSegment"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:hint="Business Segment"
                                    android:imeOptions="actionNext"
                                    android:singleLine="true"
                                    android:textColor="#111"
                                    android:textColorHint="@color/hint"
                                    android:textSize="12dp"
                                    android:clickable="true"
                                    android:editable="false"
                                    android:focusable="false"/>

                            </android.support.design.widget.TextInputLayout>

                        </LinearLayout>

                        <!--website-->
                        <android.support.design.widget.TextInputLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <EditText
                                android:id="@+id/edt_website"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:hint="Website"
                                android:singleLine="true"
                                android:text=""
                                android:textColor="#111"
                                android:textSize="12sp"
                                android:visibility="visible" />

                        </android.support.design.widget.TextInputLayout>

                        <!--source-->
                        <LinearLayout
                            android:id="@+id/ln_source"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center|left"
                            android:layout_weight="1"
                            android:gravity="center|left"
                            android:orientation="horizontal"
                            android:visibility="visible">

                            <android.support.design.widget.TextInputLayout
                                android:id="@+id/tv_source"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                                <AutoCompleteTextView
                                    android:id="@+id/spinner_source"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_weight="1"
                                    android:imeOptions="actionNext"
                                    android:singleLine="true"
                                    android:hint="Source"
                                    android:textColor="#111"
                                    android:textColorHint="@color/hint"
                                    android:textSize="12dp"
                                    android:clickable="true"
                                    android:editable="false"
                                    android:focusable="false"/>
                            </android.support.design.widget.TextInputLayout>

                        </LinearLayout>

                        <!--select Prefrence-->
                        <LinearLayout
                            android:id="@+id/ln_reference"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="center|left"
                            android:orientation="vertical"
                            android:visibility="visible">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center|left"
                                android:gravity="center|left"
                                android:orientation="horizontal"
                                android:visibility="gone">

                                <android.support.design.widget.TextInputLayout
                                    android:id="@+id/tv_reference"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">

                                    <AutoCompleteTextView
                                        android:id="@+id/spinner_reference"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:singleLine="true"
                                        android:hint="Select Reference"
                                        android:textColor="#111"
                                        android:textColorHint="@color/hint"
                                        android:textSize="12dp"
                                        android:clickable="true"
                                        android:editable="false"
                                        android:focusable="false"/>

                                </android.support.design.widget.TextInputLayout>

                            </LinearLayout>

                            <!--select-->
                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center|left"
                                android:gravity="center|left"
                                android:orientation="horizontal"
                                android:visibility="gone">

                                <android.support.design.widget.TextInputLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content">

                                    <AutoCompleteTextView
                                        android:id="@+id/spinner_refrenceoption"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:imeOptions="actionNext"
                                        android:singleLine="true"
                                        android:hint="Select"
                                        android:textColor="#111"
                                        android:textColorHint="@color/hint"
                                        android:textSize="12dp"
                                        android:clickable="true"
                                        android:editable="false"
                                        android:focusable="false"/>

                                </android.support.design.widget.TextInputLayout>



                            </LinearLayout>
                        </LinearLayout>

                        <!--notes-->
                        <LinearLayout
                            android:id="@+id/ln_Notes"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_gravity="left|top"
                            android:focusableInTouchMode="true"
                            android:gravity="left|top"
                            android:orientation="vertical"
                            android:visibility="visible">

                            <android.support.design.widget.TextInputLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content">

                                <EditText
                                    android:id="@+id/edt_Notes"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="3dp"
                                    android:layout_weight="1"
                                    android:hint="Notes"
                                    android:inputType="textMultiLine"
                                    android:text=""
                                    android:textColor="#111"
                                    android:textSize="13sp" />

                            </android.support.design.widget.TextInputLayout>


                        </LinearLayout>
                    </LinearLayout>

                    <!--headcnt-->
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:padding="5dp"
                        android:visibility="visible">

                        <TextView
                            android:id="@+id/headContact"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentStart="true"
                            android:layout_centerVertical="true"
                            android:gravity="center"
                            android:paddingLeft="5sp"
                            android:text="Contact"
                            android:textColor="#ffffff"
                            android:textSize="13sp"
                            android:textStyle="bold" />

                        <ImageView
                            android:id="@+id/ExpandContact"
                            android:layout_width="25dp"
                            android:layout_height="25dp"
                            android:layout_alignParentRight="true"
                            android:layout_marginRight="3sp"
                            android:background="@drawable/ic_add"
                            android:visibility="gone" />
                    </RelativeLayout>

                    <!--contact -->
                    <LinearLayout
                        android:id="@+id/llContact"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:focusableInTouchMode="true"
                        android:orientation="vertical"
                        android:visibility="visible">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:background="#6fa2ca"
                            android:orientation="horizontal"
                            android:padding="5dp">

                            <TextView

                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Contact Details"
                                android:textSize="15sp"
                                android:textColor="@color/white"
                                android:textStyle="bold"/>

                            <View
                                android:layout_width="match_parent"
                                android:layout_height="1dp"
                                android:background="#6fa2ca"
                                android:visibility="gone"/>

                        </LinearLayout>

                        <!--add contact layout-->
                        <LinearLayout
                            android:id="@+id/llAddContact"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:focusableInTouchMode="true"
                            android:orientation="vertical"
                            android:visibility="visible"
                            android:layout_margin="5dp"
                            android:padding="5dp"
                            android:background="@drawable/border_lightblue"
                            android:elevation="2dp">

                            <LinearLayout
                                android:id="@+id/ln_Contact"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center|left"
                                android:gravity="center|left"
                                android:orientation="horizontal"
                                android:visibility="visible">

                                <android.support.design.widget.TextInputLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
