import java.util.ArrayList;

public class CalendarCollection {
    public String NextActionDateTime="";
    public String CountSales="";
    public String CountCollection="";
    public String CountFeedBack="";

    public static ArrayList<CalendarCollection> date_collection_arr;

    public CalendarCollection() {
    }

    public CalendarCollection(String nextActionDateTime, String countSales, String countCollection, String countFeedBack) {
        NextActionDateTime = nextActionDateTime;
        CountSales = countSales;
        CountCollection = countCollection;
        CountFeedBack = countFeedBack;
    }

    public String getNextActionDateTime() {
        return NextActionDateTime;
    }

    public void setNextActionDateTime(String nextActionDateTime) {
        NextActionDateTime = nextActionDateTime;
    }

    public String getCountSales() {
        return CountSales;
    }

    public void setCountSales(String countSales) {
        CountSales = countSales;
    }

    public String getCountCollection() {
        return CountCollection;
    }

    public void setCountCollection(String countCollection) {
        CountCollection = countCollection;
    }

    public String getCountFeedBack() {
        return CountFeedBack;
    }

    public void setCountFeedBack(String countFeedBack) {
        CountFeedBack = countFeedBack;
    }

    public static ArrayList<CalendarCollection> getDate_collection_arr() {
        return date_collection_arr;
    }

    public static void setDate_collection_arr(ArrayList<CalendarCollection> date_collection_arr) {
        CalendarCollection.date_collection_arr = date_collection_arr;
    }
}