<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.vritti.sales.activity.AddNewCustProspect">

    <android.support.design.widget.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:theme="@style/AppTheme"
        android:background="#FF4112"
        android:visibility="visible">

        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar1"
            android:layout_width="match_parent"
            android:layout_height="?android:attr/actionBarSize"
            android:background="#FF4112"
            app:popupTheme="@style/AppTheme">

            <ProgressBar
                android:id="@+id/toolbar_progress_Assgnwork"
                style="?android:attr/progressBarStyle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                android:layout_marginRight="8dp"
                android:indeterminateDrawable="@drawable/spinner"
                android:visibility="gone" />
        </android.support.v7.widget.Toolbar>

    </android.support.design.widget.AppBarLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:background="#6E6F72"
        android:orientation="horizontal"
        android:padding="5dp"
        android:visibility="gone">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center|left"
            android:text="Add New Customer"
            android:textColor="#fff"
            android:textSize="16sp"
            android:layout_weight="0.6"/>

        <ImageView
            android:id="@+id/btnnewcust"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:src="@drawable/tbuds_add_custmr"
            android:layout_gravity="center|right"
            android:layout_weight="0.4"
            android:visibility="gone"/>

        <ImageView
            android:id="@+id/btnprospsetting"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:src="@drawable/tbuds_settings"
            android:layout_gravity="center|right"
            android:layout_weight="0.4"
            android:visibility="gone"/>

    </LinearLayout>

    <include
        android:id="@+id/tbar"
        layout="@layout/tbuds_common_toolbar"
        android:visibility="gone" />

    <LinearLayout
           android:id="@+id/lay_addnewcust"
           android:layout_width="match_parent"
           android:layout_height="0dp"
           android:orientation="vertical"
           android:paddingLeft="5dp"
        android:layout_weight="9"
           android:visibility="visible">

           <ScrollView
               android:layout_width="match_parent"
               android:layout_height="wrap_content"
               android:layout_above="@+id/lay2"
               android:layout_below="@+id/lay1"
               android:layout_marginLeft="5dp"
               android:layout_marginRight="5dp"
               android:isScrollContainer="false"
               android:orientation="vertical"
               android:scrollbars="none"
               android:visibility="visible">

               <LinearLayout
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content"
                   android:layout_marginTop="5dp"
                   android:focusableInTouchMode="true"
                   android:orientation="vertical">

                   <!--<EditText
                       android:id="@+id/edt_first_name"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:background="@drawable/business_edit_text"
                       android:hint="First Name"
                       android:padding="10dp"
                       android:singleLine="true"
                       android:text=""
                       android:textColorHint="@color/hint"
                       android:textSize="14dp"
                       android:visibility="visible" />

                   <EditText
                       android:id="@+id/edt_middle_name"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:layout_marginTop="5dp"
                       android:background="@drawable/business_edit_text"
                       android:hint="Middle Name "
                       android:padding="10dp"

                       android:singleLine="true"
                       android:text=""
                       android:textColor="@color/black"
                       android:textColorHint="@color/hint"
                       android:textSize="14dp"
                       android:visibility="visible" />

                   <EditText
                       android:id="@+id/edt_last_name"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:layout_marginTop="5dp"
                       android:background="@drawable/business_edit_text"
                       android:hint="Last Name"
                       android:padding="10dp"
                       android:singleLine="true"
                       android:text=""
                       android:textColor="@color/black"

                       android:textColorHint="@color/hint"
                       android:textSize="14dp"
                       android:visibility="visible" />-->

                   <!--name-->
                   <LinearLayout
                       android:id="@+id/layname"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:orientation="horizontal"
                       android:background="@drawable/business_edit_text">

                   <EditText
                       android:id="@+id/edt_name"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:background="@drawable/btn_border_button_white"
                       android:hint="Enter Name"
                       android:paddingLeft="5dp"
                       android:paddingRight="10dp"
                       android:paddingBottom="5dp"
                       android:paddingTop="5dp"
                       android:layout_marginLeft="5dp"
                       android:layout_marginTop="2dp"
                       android:layout_marginBottom="2dp"
                       android:layout_marginRight="5dp"
                       android:singleLine="true"
                       android:text=""
                       android:inputType="text"
                       android:textColorHint="@color/hint"
                       android:textSize="14dp"
                       android:visibility="visible"
                       android:layout_weight="0.5"/>

                       <ImageView
                           android:id="@+id/img_man_name"
                           android:layout_width="wrap_content"
                           android:layout_height="7dp"
                           android:src="@drawable/starred1"
                           android:paddingRight="5dp"
                           android:layout_marginTop="5dp"
                           android:visibility="visible"/>

                   </LinearLayout>

                   <!--email-->
                   <EditText
                       android:id="@+id/edt_email"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:layout_marginTop="5dp"
                       android:background="@drawable/business_edit_text"
                       android:hint="Enter Email"
                       android:paddingLeft="10dp"
                       android:paddingRight="10dp"
                       android:paddingBottom="8dp"
                       android:paddingTop="8dp"
                       android:singleLine="true"
                       android:text=""
                       android:inputType="textEmailAddress"
                       android:textColor="@color/black"
                       android:textColorHint="@color/hint"

                       android:textSize="14dp"
                       android:visibility="visible" />

                   <!--mob-->
                   <LinearLayout
                       android:id="@+id/laymob"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:orientation="horizontal"
                       android:layout_marginTop="5dp"
                       android:background="@drawable/business_edit_text">

                       <EditText
                           android:id="@+id/edt_mobile"
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"
                           android:layout_weight="1"
                           android:background="@drawable/btn_border_button_white"
                           android:hint="Enter Mobile No"
                           android:imeOptions="actionNext"
                           android:inputType="number"
                           android:maxLength="10"
                           android:paddingLeft="5dp"
                           android:paddingRight="10dp"
                           android:paddingBottom="5dp"
                           android:paddingTop="5dp"
                           android:layout_marginLeft="5dp"
                           android:layout_marginTop="2dp"
                           android:layout_marginBottom="2dp"
                           android:layout_marginRight="5dp"
                           android:singleLine="true"
                           android:text=""
                           android:textColor="@color/black"
                           android:textColorHint="@color/hint"
                           android:textSize="14dp"
                           android:visibility="visible" />

                       <ImageView
                           android:id="@+id/img_man_mob"
                           android:layout_width="wrap_content"
                           android:layout_height="7dp"
                           android:src="@drawable/starred1"
                           android:paddingRight="5dp"
                           android:layout_marginTop="5dp"
                           android:visibility="visible"/>

                   </LinearLayout>

                   <!--telphone-->
                   <EditText
                       android:id="@+id/edt_telephone"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:layout_marginTop="5dp"
                       android:layout_weight="1"
                       android:background="@drawable/business_edit_text"
                       android:hint="Enter Phone No"
                       android:imeOptions="actionNext"
                       android:inputType="number"
                       android:padding="10dp"
                       android:singleLine="true"
                       android:text=""
                       android:textColor="@color/black"
                       android:textColorHint="@color/hint"
                       android:textSize="14dp"
                       android:visibility="gone" />

                   <!--gender-->
                   <LinearLayout
                       android:id="@+id/ln_gender"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:layout_gravity="center|left"
                       android:layout_marginTop="5dp"
                       android:background="@drawable/business_edit_text"
                       android:gravity="center|left"

                       android:orientation="horizontal"
                       android:padding="10dp"
                       android:visibility="gone">

                       <TextView
                           android:id="@+id/txt_gender"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:text="Gender"
                           android:textColor="@color/black"
                           android:textSize="15dp" />

                       <RadioGroup
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"
                           android:layout_marginLeft="10dp"
                           android:layout_toRightOf="@+id/txt_gender"
                           android:orientation="horizontal">

                           <android.support.v7.widget.AppCompatRadioButton
                               android:id="@+id/radio_male"
                               android:layout_width="wrap_content"
                               android:layout_height="wrap_content"
                               android:buttonTint="@color/colorPrimary"
                               android:checked="true"
                               android:text="Male"
                               android:textColor="@color/black" />

                           <android.support.v7.widget.AppCompatRadioButton
                               android:id="@+id/radio_female"
                               android:layout_width="wrap_content"
                               android:layout_height="wrap_content"
                               android:layout_marginLeft="5dp"
                               android:buttonTint="@color/colorPrimary"
                               android:text="Female"
                               android:textColor="@color/black" />

                           <android.support.v7.widget.AppCompatRadioButton
                               android:id="@+id/radio_transgender"
                               android:layout_width="wrap_content"
                               android:layout_height="wrap_content"
                               android:layout_marginLeft="5dp"
                               android:buttonTint="@color/colorPrimary"
                               android:text="Transgender"
                               android:textColor="@color/black" />
                       </RadioGroup>

                   </LinearLayout>

                   <!--date-->
                   <LinearLayout
                       android:id="@+id/lay_Date"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:layout_gravity="center|left"
                       android:layout_marginTop="5dp"
                       android:background="@drawable/business_edit_text"
                       android:gravity="center|left"
                       android:orientation="horizontal"
                       android:padding="10dp"
                       android:visibility="gone">

                       <TextView

                           android:id="@+id/txtDate"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:text="Birth Date:"
                           android:textColor="@color/black"
                           android:textSize="15dp" />

                       <EditText
                           android:id="@+id/editTextDate"
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"
                           android:layout_marginLeft="10dp"
                           android:layout_marginRight="10dp"
                           android:layout_weight="1"
                           android:background="@drawable/business_edit_text"
                           android:cursorVisible="false"
                           android:editable="false"
                           android:focusable="false"
                           android:imeOptions="actionNext"
                           android:padding="8dp"
                           android:singleLine="true"
                           android:text=""
                           android:textSize="14dp"
                           android:visibility="visible" />

                       <ImageView
                           android:id="@+id/img_birth_date"
                           android:layout_width="25dp"
                           android:layout_height="25dp"
                           android:layout_marginRight="40dp"
                           android:src="@drawable/calender" />
                   </LinearLayout>

                   <!--marritalstatus-->
                   <LinearLayout
                       android:id="@+id/ln_marriddstatus"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:layout_gravity="center|left"
                       android:layout_marginTop="5dp"
                       android:background="@drawable/business_edit_text"
                       android:gravity="center|left"
                       android:orientation="horizontal"
                       android:padding="10dp"
                       android:visibility="gone">

                       <TextView
                           android:id="@+id/tv_marriedstatus"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:text="Marital Status"
                           android:textColor="@color/black"
                           android:textSize="15dp" />

                       <LinearLayout
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"
                           android:layout_gravity="center|left"
                           android:layout_marginLeft="10dp"
                           android:gravity="center|left"
                           android:orientation="horizontal">

                           <RadioGroup
                               android:layout_width="match_parent"
                               android:layout_height="wrap_content"
                               android:layout_marginLeft="10dp"
                               android:layout_toRightOf="@+id/txt_gender"
                               android:orientation="horizontal"
                               android:visibility="visible">

                               <android.support.v7.widget.AppCompatRadioButton
                                   android:id="@+id/radionutton_single"
                                   android:layout_width="wrap_content"
                                   android:layout_height="wrap_content"
                                   android:buttonTint="@color/colorPrimary"

                                   android:checked="true"
                                   android:text="Single"
                                   android:textColor="@color/black"
                                   android:visibility="visible" />

                               <android.support.v7.widget.AppCompatRadioButton
                                   android:id="@+id/radionutton_married"
                                   android:layout_width="wrap_content"
                                   android:layout_height="wrap_content"
                                   android:layout_marginLeft="5dp"

                                   android:buttonTint="@color/colorPrimary"
                                   android:text="Married"
                                   android:textColor="@color/black"
                                   android:visibility="visible" />
                           </RadioGroup>
                       </LinearLayout>
                   </LinearLayout>

                   <!--spousename-->
                   <LinearLayout
                       android:id="@+id/len_spouse_name"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:orientation="vertical"
                       android:visibility="gone">

                       <EditText
                           android:id="@+id/edt_spouse_name"
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"
                           android:layout_marginTop="5dp"
                           android:background="@drawable/business_edit_text"
                           android:hint="Spouse Name"
                           android:padding="10dp"
                           android:singleLine="true"
                           android:text=""
                           android:textColor="@color/black"
                           android:textColorHint="@color/hint"
                           android:textSize="14dp" />

                       <LinearLayout
                           android:id="@+id/lay_anniversary"
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"
                           android:layout_gravity="center|left"
                           android:layout_marginTop="5dp"
                           android:background="@drawable/business_edit_text"
                           android:gravity="center|left"
                           android:orientation="horizontal"
                           android:padding="10dp">

                           <TextView

                               android:id="@+id/txt_anniversary"
                               android:layout_width="wrap_content"
                               android:layout_height="wrap_content"
                               android:text="Anniversary :"
                               android:textColor="@color/black"
                               android:textSize="16dp" />

                           <EditText
                               android:id="@+id/edit_anni_date"
                               android:layout_width="match_parent"
                               android:layout_height="wrap_content"
                               android:layout_marginLeft="10dp"
                               android:layout_marginRight="10dp"
                               android:layout_weight="1"
                               android:background="@drawable/business_edit_text"
                               android:cursorVisible="false"
                               android:editable="false"
                               android:focusable="false"
                               android:imeOptions="actionNext"
                               android:padding="8dp"
                               android:singleLine="true"
                               android:text=""
                               android:textSize="14dp" />

                           <ImageView
                               android:id="@+id/img_anniversary"
                               android:layout_width="25dp"
                               android:layout_height="25dp"
                               android:layout_marginRight="40dp"
                               android:src="@drawable/calender" />
                       </LinearLayout>
                   </LinearLayout>

                   <!--experience-->
                   <EditText
                       android:id="@+id/edt_Experience"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:layout_marginTop="5dp"
                       android:background="@drawable/business_edit_text"
                       android:hint="Enter Experience"
                       android:maxLines="3"
                       android:padding="10dp"
                       android:text=""
                       android:textColor="@color/black"
                       android:textColorHint="@color/hint"
                       android:textSize="14dp"
                       android:visibility="gone" />

                   <!--qualification-->
                   <LinearLayout
                       android:id="@+id/ln_qualification"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:layout_gravity="center|left"
                       android:layout_marginTop="5dp"
                       android:layout_weight="1"
                       android:background="@drawable/business_edit_text"
                       android:gravity="center|left"
                       android:orientation="horizontal"
                       android:visibility="gone"
                       android:padding="5dp">

                       <TextView
                           android:id="@+id/tv_qualification"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:layout_marginLeft="5dp"
                           android:text="Qualification :"
                           android:textColor="@color/black"
                           android:textSize="14dp" />

                       <com.toptoche.searchablespinnerlibrary.SearchableSpinner
                           android:id="@+id/spinner_qualification"
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"
                           android:layout_marginLeft="5dp"
                           android:layout_weight="1"
                           android:entries="@array/qualification"
                           android:imeOptions="actionNext"
                           android:textColor="#000000"
                           android:textColorHint="@color/hint"
                           android:textSize="16sp" />
                   </LinearLayout>

                   <!--address-->
                   <EditText
                       android:id="@+id/edt_address"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:layout_marginTop="5dp"
                       android:background="@drawable/business_edit_text"
                       android:hint="Enter Address"
                       android:maxLines="3"
                       android:padding="10dp"
                       android:inputType="text"
                       android:textColor="@color/black"
                       android:textColorHint="@color/hint"
                       android:textSize="15dp" />

                   <!--state-->
                   <LinearLayout
                       android:id="@+id/ln_state"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:layout_gravity="center|left"
                       android:layout_marginTop="5dp"
                       android:layout_weight="1"
                       android:background="@drawable/business_edit_text"
                       android:gravity="center|left"
                       android:orientation="horizontal"
                       android:padding="5dp">

                       <TextView
                           android:id="@+id/tv_state"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:layout_marginLeft="5dp"
                           android:text="State :"
                           android:textSize="14dp"
                           android:visibility="gone" />
                       <!--android:textColor="@color/black"-->

                       <!--<com.toptoche.searchablespinnerlibrary.SearchableSpinner
                           android:id="@+id/spinner_state"
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"
                           android:layout_marginLeft="5dp"
                           android:layout_weight="1"
                           android:spinnerMode="dialog"/>-->

                       <AutoCompleteTextView
                           android:id="@+id/spinner_state"
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"
                           android:layout_marginLeft="5dp"
                           android:layout_weight="1"
                           android:spinnerMode="dialog"
                           android:hint="Select State"
                           android:textSize="14dp"
                           android:inputType="text"
                           android:textColor="@color/black"
                           android:textColorHint="@color/hint"
                           android:background="@drawable/edittext_boder"/>

                   </LinearLayout>

                   <!--city-->
                   <LinearLayout
                       android:id="@+id/ln_city"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:layout_gravity="center|left"
                       android:layout_marginTop="5dp"
                       android:layout_weight="1"
                       android:background="@drawable/business_edit_text"
                       android:gravity="center|left"
                       android:orientation="horizontal"
                       android:padding="5dp">

                       <TextView
                           android:id="@+id/tv_city"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:layout_marginLeft="5dp"
                           android:text="City :"
                           android:textSize="14dp"
                           android:visibility="gone"/>
                       <!--android:textColor="@color/black"-->

                       <!-- <com.toptoche.searchablespinnerlibrary.SearchableSpinner
                            android:id="@+id/eAutoCity"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="5dp"
                            android:hint="Select City"
                            android:imeOptions="actionNext"
                            android:singleLine="true"
                            android:textColor="@color/black"
                            android:textColorHint="@color/hint"
                            android:textSize="16dp" />-->

                       <AutoCompleteTextView
                           android:id="@+id/eAutoCity"
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"
                           android:layout_marginLeft="5dp"
                           android:hint="Select City"
                           android:imeOptions="actionNext"
                           android:singleLine="true"
                           android:inputType="text"
                           android:textColor="@color/black"
                           android:textColorHint="@color/hint"
                           android:background="@drawable/edittext_boder"
                           android:textSize="14dp" />

                   </LinearLayout>

                   <!--postalcode-->
                   <EditText
                       android:id="@+id/edt_postalcode"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:layout_marginTop="5dp"
                       android:background="@drawable/business_edit_text"
                       android:hint="Enter Postal Code"
                       android:inputType="number"
                       android:maxLength="6"
                       android:padding="10dp"
                       android:textColor="@color/black"
                       android:textColorHint="@color/hint"
                       android:textSize="14dp" />

                   <!--country-->
                   <LinearLayout
                       android:id="@+id/ln_country"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:layout_gravity="center|left"
                       android:layout_marginTop="5dp"
                       android:layout_weight="1"
                       android:background="@drawable/business_edit_text"
                       android:gravity="center|left"
                       android:orientation="horizontal"
                       android:padding="5dp"
                       android:visibility="gone">

                       <TextView
                           android:id="@+id/tv_country"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:layout_marginLeft="5dp"
                           android:text="Country :"
                           android:textColor="@color/black"
                           android:textSize="14dp"
                           android:visibility="visible" />

                       <com.toptoche.searchablespinnerlibrary.SearchableSpinner
                           android:id="@+id/spinner_country"
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"
                           android:layout_marginLeft="5dp"
                           android:layout_weight="1"
                           android:spinnerMode="dialog"></com.toptoche.searchablespinnerlibrary.SearchableSpinner>
                   </LinearLayout>

                   <!--territory-->
                   <LinearLayout
                       android:id="@+id/ln_territory"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:layout_marginTop="5dp"
                       android:layout_weight="1"
                       android:background="@drawable/business_edit_text"
                       android:orientation="horizontal"
                       android:padding="5dp"
                       android:visibility="gone">

                       <TextView
                           android:id="@+id/tv_territory"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:layout_marginLeft="5dp"
                           android:text="Territory :"
                           android:textColor="@color/black"
                           android:textSize="14dp" />

                       <com.toptoche.searchablespinnerlibrary.SearchableSpinner
                           android:id="@+id/eAutoTerritory"
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"
                           android:layout_marginLeft="5dp"
                           android:hint="Territory"
                           android:imeOptions="actionNext"
                           android:singleLine="true"
                           android:textColor="#000000"
                           android:textColorHint="@color/hint"
                           android:textSize="16dp" />
                   </LinearLayout>

                   <!--product-->
                   <LinearLayout
                       android:id="@+id/ln_Product"
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
