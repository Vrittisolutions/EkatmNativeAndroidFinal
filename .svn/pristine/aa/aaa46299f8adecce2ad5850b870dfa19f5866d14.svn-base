    useLibrary 'org.apache.http.legacy'
    defaultConfig {
        applicationId "com.vritti.ekatm"
        minSdkVersion 15
        targetSdkVersion 27
        versionCode 1
        versionName "1.0"
        testInstrumentationRunner "android.support.test.runner.AndroidJUnitRunner"

        dexOptions {
            javaMaxHeapSize "2g"
        }

        defaultConfig {
            applicationId "vworkbench7.vritti.com.vworkbench7"
            minSdkVersion 16
            targetSdkVersion 27
            versionCode 46
            versionName "3.0.16"
            multiDexEnabled true
            /*dexOptions {
                javaMaxHeapSize "2g"
            }*/
        }
        buildTypes {
            release {
                /*  minifyEnabled true
                proguardFiles getDefaultProguardFile('proguard-android.txt'), 'proguard-rules.pro'*/
            }
        }

        flavorDimensions "default"

        productFlavors {
            Vwb {
                minSdkVersion 16
                applicationId 'vworkbench7.vritti.com.vworkbench7'
                targetSdkVersion 27
                versionCode 80
                versionName '5.1.7'
            }
            /*  CRM {
                  minSdkVersion 16
                  applicationId 'vcrm7.vritti.com.vcrm7'
                  targetSdkVersion 27
                  versionCode 47
                  versionName '4.0.4'
              }*/
            PM {
                minSdkVersion 16
                applicationId 'practice.vritti.com'
                targetSdkVersion 27
                versionCode 10
                versionName '1.0.9'
            }
            MilkRun {
                minSdkVersion 16
                applicationId 'vworkbench7.vritti.com.milk.run'
                targetSdkVersion 28
                versionCode 01
                versionName '1.0.0'
            }

            Sahara{
                minSdkVersion 16
                applicationId 'sahara.vritti.com'
                targetSdkVersion 28
                versionCode 01
                versionName '1.0.0'
            }
        }

        repositories {
            maven {
                url "https://jitpack.io"
            }
            maven {
                url uri('mint-plugin-repo-5.2.5')
            }
        }


    }
    lintOptions {
        checkReleaseBuilds false // Add this
        abortOnError false
    }
    buildToolsVersion '27.0.3'
}
configurations {
    all*.exclude group: 'commons-io'
}

